<pclass="blockquote text-center">
<%= link_to 'New Article', '/articles/new' %>
</p>

<ul>
  <% @articles.each do |article| %>
    <li>
      <%= article.title %><br />
      <%= article.content %><br/>
      <%= link_to 'Edit Article', edit_article_path(article) %><br/>
      <%= link_to 'Show', article_path(article)%>
    </li>
  <% end %>
</ul>
