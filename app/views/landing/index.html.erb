<p>
<%= link_to 'new article', '/articles/new' %>
</p>

<ul>
  <% @articles.each do |article| %>
    <li>
      <%= article.title %><br />
      <%= article.content %>
      <%= link_to 'Edit', edit_article_path(article) %>
      <%= link_to 'Show', article_path(article)%>
    </li>
  <% end %>
</ul>
